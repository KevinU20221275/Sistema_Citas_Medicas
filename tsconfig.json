{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*" : ["/src/*"],
      "@components/*" : ["/components/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}