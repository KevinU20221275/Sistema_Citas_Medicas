{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*" : ["src/*"],
      "@components/*" : ["src/components/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "module": "CommonJS",
  "esModuleInterop": true,
  "moduleResolution": "Node",
  "types": ["jest", "node"]
}