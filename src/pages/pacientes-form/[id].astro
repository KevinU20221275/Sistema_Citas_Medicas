---
import Layout from "src/layouts/Layout.astro";
import { PacienteForm } from "src/components/pacientes/PacienteForm";

interface Props {
    id: string
}

const {id} = Astro.params
---

<Layout title={`Citas Medicas | ${id ? 'Actualizar' : 'Agregar'} Paciente`}>
    <PacienteForm id={id}  client:visible />
</Layout>